{
  "name": "veat",
  "version": "0.2.30",
  "description": "a tips list",
  "main": "./lib/veat.js",
  "scripts": {
    "dev-test": "npm run icon && npm run rollup-dev ",
    "build-test": "npm run icon && npm run rollup-prod",
    "rollup-dev": "rollup -c --environment MODE:development",
    "rollup-prod": "rollup -c --environment MODE:production",
    "build": "npm run icon && npm run tpl && npm run tsc && npm run core",
    "icon": "node src/icon_modules/index.js",
    "core": "node src/core_modules/index.js",
    "tpl": "node src/tpl_modules/index.js",
    "tsc": "tsc"
  },
  "keywords": [],
  "type": "module",
  "author": "gonjwen",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/plugin-url": "^6.1.0",
    "cssnano": "^5.0.17",
    "postcss": "^8.4.6",
    "postcss-preset-env": "^7.3.3",
    "rollup": "^2.67.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "exports": {
    "./core": "./lib/index.js",
    "./core/dialog": "./lib/dialog/index.js",
    "./core/iart": "./lib/iart/index.js",
    "./core/iform": "./lib/iform/index.js",
    "./core/notify": "./lib/notify/index.js",
    "./core/pace": "./lib/pace/index.js",
    "./core/signature": "./lib/signature/index.js",
    "./core/toast": "./lib/toast/index.js",
    "./core/utils": "./lib/utils/index.js",
    "./core/css/icons": "./lib/icons/index.css",
    "./core/css/outlined/*": "./lib/icons/outlined/*.css",
    "./core/css/filled/*": "./lib/icons/filled/*.css",
    "./core/css/shared/*": "./lib/icons/shared/*.css"
  },
  "files": [
    "core",
    "lib",
    "styles",
    "art.md",
    "icon.md",
    "veat.d.ts"
  ]
}
