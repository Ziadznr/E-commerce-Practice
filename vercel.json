{
    "projects": [{
        "src": "client/",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
    }],
    "builds": [{
        "src": "client/package.json",
        "use": "@vercel/static-build"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/client/$1"
    }]
}